# Add `/usr/local/bin to $PATH
export PATH="/usr/local/bin:$PATH";

# Add `~/bin` to the `$PATH`
export PATH="$HOME/bin:$PATH";

# Add `~/.rvm/bin` to the `$PATH`
export PATH="$HOME/.rvm/bin:$PATH";

# Add `~/anaconda2/bin` to the `$PATH`
export PATH="$HOME/anaconda2/bin:$PATH";

